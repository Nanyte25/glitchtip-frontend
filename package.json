{
  "name": "glitchtip-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "start-prod-no-static": "ng serve --c prod-no-static",
    "build": "ng build",
    "build-prod": "ng build -c prod-no-static,production --deploy-url static/ --progress false",
    "test": "ng test",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:run\" --kill-others --success first",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@angular/animations": "~11.1.2",
    "@angular/cdk": "~11.1.2",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/forms": "~11.1.2",
    "@angular/material": "^11.1.2",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/platform-server": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@braintree/sanitize-url": "^5.0.0",
    "@egoistdeveloper/operating-system-logos": "^1.0.0",
    "@sentry/angular": "^6.1.0",
    "@stripe/stripe-js": "^1.12.1",
    "browser-logos": "github:alrra/browser-logos#69.0.4",
    "date-fns": "^2.17.0",
    "ngx-markdown": "^11.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "~11.1.4",
    "@angular/compiler-cli": "~11.1.2",
    "@angular/language-service": "~11.1.2",
    "@babel/core": "^7.7.2",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/angular": "^6.1.15",
    "@types/braintree__sanitize-url": "^4.1.0",
    "@types/deep-freeze": "^0.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.0.6",
    "codelyzer": "^6.0.1",
    "concurrently": "^5.1.0",
    "cypress": "^6.4.0",
    "cypress-localstorage-commands": "^1.4.0",
    "deep-freeze": "0.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "wait-on": "^4.0.1",
    "webpack-bundle-analyzer": "^4.4.0"
  }
